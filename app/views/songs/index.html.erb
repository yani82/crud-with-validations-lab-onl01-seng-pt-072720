<h1>Songs</h1>

<div>
    <%= link_to 'Create Song', new_song_path %>
</div>

<table>
    <thead>
    <tr>
        <th>Title</th>
        <th>Release Year</th>
        <th>Genre</th>
        <th>Artist Name</th>
    </tr>
    </thead>
    <tbody>
        <% @songs.each do |s| %> 
        <tr>
            <td><%= s.title %></td>
            <td><%= s.release_year %></td>
            <td><%= s.genre %></td>
            <td><%= s.artist_name %></td>
            <td>
                <%= link_to 'View', s %>
                <%= link_to 'Edit', edit_song_path(s) %>
                <%= link_to 'Delete', s, method: :delete, data: { confirm: 'For real?' } %>
                </td> 
        </tr>
        <% end %>
    </tbody>
</table>